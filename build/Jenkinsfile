pipeline {
  agent any

   environment {
      REGIO = "EMEA"
      BRANCH = "PROD"
      CONFIG = "TestConf/${REGION}/TestConfigs_${BRANCH}"
   }

  stages {
   stage ("clean") {
     steps {
       echo "clean"
     }
   }

   stage ("test") {
     steps {
       script {
		def b = load "test.groovy"
		b.doit()
	 }
       }
     }
   }
}
