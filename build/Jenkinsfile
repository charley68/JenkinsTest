pipeline {
  agent any

   environment {
      REGIO = "EMEA"
      BRANCH = "PROD"
      CONFIG = "TestConf/${REGION}/TestConfigs_${BRANCH}"
   }

  stages {
   stage ("clean") {
     steps {
       echo "clean"
     }
   }

   stage ("test") {
     steps {
       script {
		echo "ROOT:  ${rootDir}"
		echo "ROOT@SCRIPT:  ${rootDir}@script/bob"


		def b = load "build/test.groovy"
		b.doit()
	 }
       }
     }
   }
}
